{
  "root": "build/",
  "routes": {
    "/**": "index.html"
  },
  "headers": {
    "/": {
      "Cache-Control": "no-store, no-cache"
    },
    "**.js": {
      "Cache-Control": "public, max-age=604800"
    },
    "**.css": {
      "Cache-Control": "public, max-age=604800"
    }
  },
  "https_only": true
}
